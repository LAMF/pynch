<script>
function byId(id) {
  return document.getElementById(id);
}

function vote(node) {
  var v = node.id.split(/_/);   // {'up', '123'}
  var item = v[1]; 

  // hide arrows
  byId('up_'   + item).style.visibility = 'hidden';
  byId('down_' + item).style.visibility = 'hidden';

  // ping server
  var ping = new Image();
  ping.src = node.href;

  return false; // cancel browser nav
} </script><title>Hacker News</title></head><body><center><table border=0 cellpadding=0 cellspacing=0 width="85%" bgcolor=#f6f6ef><tr><td bgcolor=#ff6600><table border=0 cellpadding=0 cellspacing=0 width="100%" style="padding:2px"><tr><td style="width:18px;padding-right:4px"><a href="http://ycombinator.com"><img src="http://ycombinator.com/images/y18.gif" width=18 height=18 style="border:1px #ffffff solid;"></img></a></td><td style="line-height:12pt; height:10px;"><span class="pagetop"><b><a href="news">Hacker News</a></b><img src="http://ycombinator.com/images/s.gif" height=1 width=10><a href="newest">new</a> | <a href="threads?id=JSig">threads</a> | <a href="newcomments">comments</a> | <a href="ask">ask</a> | <a href="jobs">jobs</a> | <a href="submit">submit</a></span></td><td style="text-align:right;padding-right:4px;"><span class="pagetop"><a href="user?id=JSig">JSig</a>&nbsp;(265)&nbsp;|&nbsp;<a href="/r?fnid=Uf6ngfMwN2">logout</a></span></td></tr></table></td></tr><tr style="height:10px"></tr><tr><td><table border=0 cellpadding=0 cellspacing=0><tr><td align=right valign=top class="title">31.</td><td><center><a id=up_2554807 onclick="return vote(this)" href="vote?for=2554807&dir=up&by=JSig&auth=99f0a06c602aaf352d173b678eade4e348b05ff0&whence=%2f%78%3f%66%6e%69%64%3d%47%77%63%4f%64%77%32%45%41%74"><img src="http://ycombinator.com/images/grayarrow.gif" border=0 vspace=3 hspace=2></a><span id=down_2554807></span></center></td><td class="title"><a href="http://blogs.forbes.com/85broads/2011/05/16/the-false-choice-between-babies-and-startups/">The False Choice Between Babies And Startups</a><span class="comhead"> (forbes.com) </span></td></tr><tr><td colspan=2></td><td class="subtext"><span id=score_2554807>46 points</span> by <a href="user?id=jemeshsu"><font color=#3c963c>jemeshsu</font></a> 12 hours ago  | <a href="/r?fnid=8ENhDlPr6J">flag</a> | <a href="item?id=2554807">30 comments</a></td></tr><tr style="height:5px"></tr><tr><td align=right valign=top class="tit